    {% block stylesheet %} {{ encore_entry_link_tags('app') }}{% endblock %}

</head>

<body>
    {% include 'header.html.twig' %}
        {% block body %}{% endblock %}
        {% block javascripts %}{{ encore_entry_script_tags('app') }}{% endblock %}
    {% include 'footer.html.twig' %}
</body>
</html>
